<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="145.0" prefWidth="381.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.NameController">
   <children>
      <Button fx:id="nextStepButton" layoutX="164.0" layoutY="75.0" mnemonicParsing="false" onAction="#codesStep" text="Далее" />
      <Label layoutX="125.0" layoutY="14.0" text="Игроки, введите имена" />
      <TextField fx:id="firstPlayerName" layoutX="14.0" layoutY="40.0" promptText="Игрок 1" />
      <TextField fx:id="secondPlayerName" alignment="CENTER_RIGHT" layoutX="213.0" layoutY="40.0" promptText="Игрок 2" />
      <Button fx:id="backButton" layoutX="14.0" layoutY="103.0" mnemonicParsing="false" onAction="#backToMain" text="Отмена" />
      <Label fx:id="errorLabel" alignment="CENTER" layoutX="102.0" layoutY="108.0" prefHeight="17.0" prefWidth="176.0" textFill="#db0000" />
   </children>
</AnchorPane>
